<?xml version="1.0" encoding="UTF-8"?>
<sbfs:Simulation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:sbfs="http://www.streambase.com/schemas/feedsim/02/SBFS" Description="">
  <SimStreams Name="WriteInput">
    <ProcessingOptions DataRatePerSecond="1000.0" DataGenerationMethod="Custom" LogToInput="false"/>
    <Schema>
      <Fields Name="time_int" DataTypeName="int" Description="">
        <Distribution GenerationMethod="Incremented" MaxValue="1000000.0" Cycle="true"/>
        <DataSourceMapping/>
      </Fields>
      <Fields Name="symbol" DataTypeName="string" Description="">
        <Distribution GenerationMethod="Enumerated" NumRandomEnumValues="1" EnumeratedDistMethod="SpecifiedValues">
          <EnumeratedDist weight="2.0" value="ORC"/>
          <EnumeratedDist weight="1.0" value="IBM"/>
          <EnumeratedDist weight="1.0" value="INTC"/>
          <EnumeratedDist weight="2.0" value="GOOG"/>
          <EnumeratedDist weight="2.0" value="YHOO"/>
          <EnumeratedDist weight="1.0" value="AAPL"/>
        </Distribution>
        <DataSourceMapping DestFieldNum="2"/>
      </Fields>
      <Fields Name="bid_price" DataTypeName="double" Description="">
        <Distribution GenerationMethod="Uniform" MinValue="25.0" MaxValue="700.0"/>
        <DataSourceMapping DestFieldNum="3"/>
      </Fields>
      <Fields Name="bid_size" DataTypeName="int" Description="">
        <Distribution GenerationMethod="Uniform" MinValue="25.0" MaxValue="100.0" ConstantValue=""/>
        <DataSourceMapping DestFieldNum="4"/>
      </Fields>
      <Fields Name="ask_price" DataTypeName="double" Description="">
        <Distribution GenerationMethod="Uniform" MinValue="25.0" MaxValue="700.0"/>
        <DataSourceMapping DestFieldNum="5"/>
      </Fields>
      <Fields Name="ask_size" DataTypeName="int" Description="">
        <Distribution GenerationMethod="Uniform" MinValue="25.0" MaxValue="120.0"/>
        <DataSourceMapping DestFieldNum="6"/>
      </Fields>
      <Fields Name="sequence" DataTypeName="int" Description="">
        <Distribution GenerationMethod="Incremented" MaxValue="1000000.0" Cycle="true"/>
        <DataSourceMapping DestFieldNum="7"/>
      </Fields>
    </Schema>
  </SimStreams>
  <SimStreams Name="ReadInput">
    <ProcessingOptions/>
    <Schema/>
  </SimStreams>
</sbfs:Simulation>
